{
  "name": "@jope-io/ecobee",
  "version": "0.1.0",
  "description": "üêù a library for interacting with ecobee devices",
  "main": "index.js",
  "scripts": {
    "test": "npx xo --quiet && npx ava",
    "tdd": "npx ava --watch",
    "coverage": "nyc npx ava",
    "coverage-html": "nyc -r html npx ava && open ./coverage/index.html",
    "lint": "npx xo",
    "lint-fix": "npx xo --fix",
    "document": "npx documentation readme index.js -f md --section=Docs"
  },
  "xo": {
    "space": true,
    "rules": {
      "camelcase": [
        "off"
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "ava": {
    "helpers": [
      "**/helpers/**/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jope-io/JopeHome-ecobee.git"
  },
  "keywords": [
    "üêù ",
    "ecobee",
    "thermostat",
    "smart",
    "house",
    "home",
    "hvac",
    "air conditioning",
    "API",
    "controller"
  ],
  "author": "Max Isom <codetheweb@icloud.com> (https://maxisom.me)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jope-io/JopeHome-ecobee/issues"
  },
  "homepage": "https://github.com/jope-io/JopeHome-ecobee#readme",
  "devDependencies": {
    "ava": "^2.1.0",
    "documentation": "^11.0.0",
    "husky": "^2.4.0",
    "nock": "^10.0.6",
    "nyc": "^14.1.1",
    "xo": "^0.24.0"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "delay": "^4.2.0",
    "got": "^9.6.0"
  }
}
